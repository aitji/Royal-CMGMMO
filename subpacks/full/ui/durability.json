				"binding_name": "#selected_item_durability_total_amount"
			},
			{
				"binding_type": "view",
				"source_property_name": "(not (#selected_item_durability_total_amount = 0))",
				"target_property_name": "#visible"
			}
		],
		"controls": [
			{
				"durability_panel_main": {
					"type": "stack_panel",
